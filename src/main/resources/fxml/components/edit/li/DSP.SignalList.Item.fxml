<HBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle fx:id="guiBrushColorIndicatorShape" arcHeight="5.0" arcWidth="5.0" height="24.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="24.0" />
      <HBox alignment="CENTER_LEFT" spacing="5.0">
         <children>
            <Label fx:id="guiNameLabel" text="\%TITLE%" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</HBox>
